import streamlit as st

st.title("ðŸ”’ Login to Age App")

st.markdown("""
    <style>
    div.stButton > button:first-child {
        background-color: #0095f6; color: white; width: 100%; border-radius: 8px; height: 3em; font-weight: bold;
    }
    </style>
    """, unsafe_allow_html=True)

